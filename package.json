{
  "name": "oae-rest",
  "main": "./lib/api.js",
  "description": "Open Academic Environment (OAE) REST client library",
  "version": "16.0.3",
  "homepage": "http://www.oaeproject.org",
  "type": "module",
  "author": {
    "name": "The Apereo Foundation",
    "email": "oae@apereo.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oaeproject/oae-rest.git"
  },
  "bugs": {
    "url": "https://github.com/oaeproject/oae-rest/issues"
  },
  "license": "ECL-2.0",
  "dependencies": {
    "ramda": "^0.27.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.18.2",
    "xo": "^0.44.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.8"
  },
  "scripts": {
    "lint": "node_modules/.bin/xo --fix --prettier --quiet 'lib/**/*.js'"
  },
  "xo": {
    "prettier": true,
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "ignores": [
      "node_modules"
    ],
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "import/extensions": "off",
      "prettier/prettier": "error",
      "node/file-extension-in-import": "off"
    }
  }
}
